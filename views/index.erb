<!doctype html !>
<html>
  <head>
    <title>Magic Clock</title>
    <link href='http://fonts.googleapis.com/css?family=Mystery+Quest' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" type="text/css" href="/css/site.css" />
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <script type="text/javascript" src="/js/jquery.ba-dotimeout.min.js"></script>
    <script type="text/javascript" src="/js/site.js"></script>
  </head>
  <body>
    <div id="clock">
      <div class="location" data-rotation="-60">Home</div>
      <div class="location" data-rotation="-20">Work</div>
      <div class="location" data-rotation="20">Pub</div>
      <div class="location" data-rotation="60">Mortal<br />peril</div>
      <div id="hand">
        <div id="face"></div>
      </div>
    </div>
  </body>
</html>
